{
  "name": "GetTables",
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/catalogs/[catalogName]/databases/[databaseName]/tables",
  "parameters": [
    {
      "name": "catalogName",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "databaseName",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "namespace",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "tableName",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "workspace",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}